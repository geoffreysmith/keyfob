      <section name="basicAuth" type="KeyFob.BasicAuthenticationConfigurationSection" xdt:Transform="Insert" />
    </configSections>
    <basicAuth>
      <credentials>
        <add username="demo" password="demo" xdt:Transform="Insert" />
      </credentials>
    </basicAuth>
  <system.webServer>
